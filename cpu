#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x9cfdb0 .scope module, "main" "main" 2 10;
 .timescale -12 -12;
v0x9dfff0_0 .net "clk", 0 0, v0x9dff50_0; 1 drivers
v0x9e00c0_0 .var "i", 15 0;
v0x9e0140 .array "input_data", 0 100, 15 0;
v0x9e01c0 .array "map", 99 0, 0 15;
E_0x9acd60 .event posedge, v0x9cf320_0;
S_0x9cfea0 .scope module, "c0" "clock" 2 21, 3 3, S_0x9cfdb0;
 .timescale -12 -12;
v0x9cf320_0 .alias "clk", 0 0, v0x9dfff0_0;
v0x9dff50_0 .var "theClock", 0 0;
    .scope S_0x9cfea0;
T_0 ;
    %set/v v0x9dff50_0, 1, 1;
    %end;
    .thread T_0;
    .scope S_0x9cfea0;
T_1 ;
    %delay 500, 0;
    %load/v 8, v0x9dff50_0, 1;
    %inv 8, 1;
    %set/v v0x9dff50_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x9cfdb0;
T_2 ;
    %vpi_call 2 12 "$dumpfile", "cpu.vcd";
    %vpi_call 2 13 "$dumpvars", 2'sb01, S_0x9cfdb0;
    %set/v v0x9e00c0_0, 0, 16;
T_2.0 ;
    %load/v 8, v0x9e00c0_0, 16;
    %mov 24, 0, 1;
   %cmpi/u 8, 99, 17;
    %jmp/0xz T_2.1, 5;
    %ix/getv 3, v0x9e00c0_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x9e01c0, 0, 16;
t_0 ;
    %load/v 8, v0x9e00c0_0, 16;
    %mov 24, 0, 16;
    %addi 8, 1, 32;
    %set/v v0x9e00c0_0, 8, 16;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
    .scope S_0x9cfdb0;
T_3 ;
    %wait E_0x9acd60;
    %vpi_call 2 28 "$readmemh", "input.data", v0x9e0140;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "engine.v";
    "clock.v";
